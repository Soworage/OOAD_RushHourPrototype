<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22"
            maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="720.0" prefWidth="1280.0">
    <children>
        <TableView fx:id="tableViewHighScore" layoutX="12.0" layoutY="52.0" prefHeight="570.0" prefWidth="577.0">
            <columns>
                <TableColumn fx:id="columnName" prefWidth="216.0" text="Name"/>
                <TableColumn fx:id="columnMoves" prefWidth="93.0" text="Zuege"/>
                <TableColumn fx:id="columnTime" prefWidth="123.0" text="Zeit"/>
                <TableColumn fx:id="columnDate" prefWidth="144.0" text="Datum"/>
            </columns>
        </TableView>
        <Button fx:id="mainMenuButton" layoutX="456.0" layoutY="634.0" mnemonicParsing="false"
                onAction="#backToMainMenuPress" prefHeight="46.0" prefWidth="130.0" text="Hauptmenü"/>
        <Label layoutX="14.0" layoutY="14.0" text="Highscore für ">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label layoutX="125.0" layoutY="14.0" text="Brett ID:">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label fx:id="boardIdLabel" layoutX="192.0" layoutY="14.0" text="0000">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Label fx:id="difficultyLabel" layoutX="237.0" layoutY="14.0" text="(Difficulty)">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
