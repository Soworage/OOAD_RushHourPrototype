<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/22">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="599.0" prefWidth="936.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="buttonStart" layoutX="319.0" layoutY="213.0" mnemonicParsing="false"
                        onAction="#onButtonPress" prefHeight="129.0" prefWidth="190.0" text="Start Game"/>
                <Button fx:id="settingsButton" layoutX="731.0" layoutY="10.0" mnemonicParsing="false"
                        onAction="#onSettingsButtonPress" prefHeight="61.0" prefWidth="77.0" text="SETTINGS"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
